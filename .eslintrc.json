{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "global-require": "off",
        "treatUndefinedAsUnspecified": true,
        "import/extensions": [
    "error",
    {
        "js": "ignorePackages"
    }
   ]
    }
}
